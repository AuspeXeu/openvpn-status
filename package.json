{
  "name": "openvpn-status",
  "homepage": "https://github.com/AuspeXeu/openvpn-status",
  "description": "A web-based application to monitor clients connected to an openvpn server.",
  "version": "4.2.13",
  "license": "MIT",
  "main": "server.js",
  "author": {
    "name": "Christian Vaas",
    "email": "christianvaas@auspex.eu",
    "url": "https://auspex.eu"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run deps && npm run ncu && npm run lint"
    }
  },
  "keywords": [
    "OpenVPN",
    "monitoring",
    "vpn"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/AuspeXeu/openvpn-status.git"
  },
  "bugs": {
    "url": "https://github.com/AuspeXeu/openvpn-status"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deps": "depcheck . --ignores=\"husky,sqlite3,@vue/cli-plugin-babel,@vue/cli-plugin-eslint,@vue/eslint-config-airbnb,vue-cli-plugin-vuetify\"",
    "ncu": "ncu"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "axios": "0.19.0",
    "body-parser": "1.19.0",
    "cron": "1.7.1",
    "express": "4.17.1",
    "maxmind": "3.1.2",
    "moment": "2.24.0",
    "nconf": "0.10.0",
    "request": "2.88.0",
    "sequelize": "5.11.0",
    "sqlite3": "4.0.9",
    "uuid": "3.3.2",
    "vue": "2.6.10",
    "vue-router": "3.0.7",
    "vuetify": "2.0.0",
    "vuex": "3.1.1",
    "ws": "7.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.9.0",
    "@vue/cli-plugin-eslint": "3.9.2",
    "@vue/cli-service": "3.9.3",
    "@vue/eslint-config-airbnb": "4.0.0",
    "depcheck": "0.8.3",
    "husky": "3.0.1",
    "npm-check-updates": "3.1.19",
    "vue-cli-plugin-vuetify": "0.6.1",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
