{
  "name": "openvpn-status",
  "homepage": "https://github.com/AuspeXeu/openvpn-status",
  "description": "A web-based application to monitor clients connected to an openvpn server.",
  "version": "4.2.19",
  "license": "MIT",
  "main": "server.js",
  "author": {
    "name": "Christian Vaas",
    "email": "christianvaas@auspex.eu",
    "url": "https://auspex.eu"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run deps && npm run ncu"
    }
  },
  "keywords": [
    "OpenVPN",
    "monitoring",
    "vpn"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/AuspeXeu/openvpn-status.git"
  },
  "bugs": {
    "url": "https://github.com/AuspeXeu/openvpn-status"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deps": "depcheck . --ignores=\"husky,sqlite3,@vue/*,eslint-plugin-import,babel-eslint,vue-template-compiler\"",
    "ncu": "ncu"
  },
  "dependencies": {
    "axios": "0.19.0",
    "body-parser": "1.19.0",
    "cron": "1.7.2",
    "express": "4.17.1",
    "maxmind": "4.0.0",
    "moment": "2.24.0",
    "nconf": "0.10.0",
    "request": "2.88.0",
    "sequelize": "5.21.2",
    "sqlite3": "4.1.0",
    "uuid": "3.3.3",
    "vue": "2.6.10",
    "vue-router": "3.1.3",
    "vuetify": "2.1.9",
    "vuex": "3.1.1",
    "ws": "7.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "4.0.5",
    "@vue/cli-service": "4.0.5",
    "@vue/eslint-config-airbnb": "4.0.1",
    "babel-eslint": "10.0.3",
    "depcheck": "0.9.0",
    "eslint": "6.6.0",
    "eslint-plugin-vue": "5.2.3",
    "husky": "3.0.9",
    "npm-check-updates": "3.2.0",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-mixed-operators": 0,
      "max-len": 0,
      "no-plusplus": 0,
      "no-return-assign": 0,
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "nonblock-statement-body-position": [
        "error",
        "below"
      ],
      "curly": [
        "error",
        "multi"
      ],
      "no-new-require": 0,
      "new-cap": 0,
      "semi": 0,
      "no-console": [
        "error",
        {
          "allow": [
            "log",
            "error"
          ]
        }
      ],
      "object-curly-spacing": 0,
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "_"
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "consistent-return": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "maintainers": [
    {
      "name": "Christian Vaas",
      "email": "chrisvaas@gmail.com"
    },
    {
      "name": "Estelle Beguin",
      "email": "beguin.stl@gmail.com"
    }
  ]
}
